# Fortnite Hub - Environment Configuration
# Copy this file to .env and fill in your values

# API Configuration
FORTNITE_API_KEY=your_api_key_here
FORTNITE_API_URL=https://fortnite-api.com/v2/

# App Configuration
APP_ENV=production
APP_DEBUG=false
APP_URL=http://localhost/Fortnite

# Cache Configuration (in seconds)
CACHE_ENABLED=true
CACHE_TTL=300
